{
  "name": "Data Syder",
  "description": "Get infomation from site",
  "version": "1.0",
  "permissions": [
    "webNavigation",
    "tabs",
    "declarativeContent",
    "activeTab",
    "<all_urls>",
    "storage",
    "notifications",
    "http://*.linkedin.com/*"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "web_accessible_resources": ["images/get_started128.png"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "128": "images/get_started16.png",
    "128": "images/get_started32.png",
    "128": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*", "https://www.google.com/*"],
      "run_at": "document_idle",
      "css": ["plugincss.css"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "images/logo.png",
    "icons/iconemail.png",
    "icons/iconphone.png",
    "icons/iconcompany.png",
    "icons/iconwebsite.png",
    "icons/iconlocate.png",
    "icons/iconsession.png",
    "icons/iconmony.png",
    "icons/iconuser.png",
    "icons/iconcalenda.png",
    "icons/iconbusiness.png",
    "icons/socialmediaicon.png",
    "icons/iconcopy.png",
    "icons/iconclose.png"
  ]
}
